{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-underscore-dangle": "off",
        "max-len": [
            "error",
            {
                "code": 120
            }
        ],
        "react/prop-types": 0,
        "react/jsx-props-no-spreading": [
            "error",
            {
                "custom": "ignore"
            }
        ]
    }
}